const videoEditorPromoTile = document.getElementsByClassName("video-editor-promo-tile")[0]

const trackShowVideoEditorPromoTile = () => {
  if (window.tracking_data) 
    window.tracking_data.merchandising_units = [...(window.tracking_data.merchandising_units || []), "sub_header_video_editor_templates"]

  if (window.combinedGtmTracking != null) 
    window.combinedGtmTracking.gtmPushTracking('merchUnitInteraction', {
      'merch_unit_interaction': 'Shown',
      'merch_unit_selection': 'sub_header_video_editor_templates_shown',
      'ui_element_location': 'sub_header_merch_spot'
    })
}

const mutationObserverCallback = (mutationsList, observer) => {
  mutationsList.forEach(mutation => {
    // Once the user hovers over the Video drop down menu, the active class is added to it
      if (mutation.attributeName === 'class' && mutation.target.className === 'nav-root__list-item nav-root__list-item--delayed nav-root__list-item--active'
        && videoEditorPromoTile) 
          trackShowVideoEditorPromoTile()
  })
}

const mutationObserver = new MutationObserver(mutationObserverCallback)

if (videoMenuItem)
  mutationObserver.observe(
    videoMenuItem,
    { attributes: true }
  )

const trackClickVideoEditorPromoTile = () => {
  if (window.tracking_data) 
    window.tracking_data.merchandising_units = [...(window.tracking_data.merchandising_units || []), "sub_header_video_editor_templates"]

  if (window.combinedGtmTracking != null) 
    window.combinedGtmTracking.gtmPushTracking('merchUnitInteraction', {
      'merch_unit_interaction': 'Click',
      'merch_unit_selection': 'sub_header_video_editor_templates_clicked',
      'ui_element_location': 'sub_header_merch_spot'
    })
}

if (videoEditorPromoTile) 
  videoEditorPromoTile.addEventListener("click", trackClickVideoEditorPromoTile)</script></li><li class="nav-root__list-item" id="photos"><a class="nav-root__link" data-nav="nav=nav_Photos" href="/fr/images">Photos</a><span class="carrot-container"><span class="carrot"></span></span><nav class="navigation-submenu navigation-submenu--hidden"><div class="navigation-submenu__photos"><button class="navigation-submenu__close-btn"></button><div class="navigation-submenu__container" id="photos"><div class="navigation-submenu__column"><div class="navigation-submenu__header">Sélection de contenus</div><div class="navigation-submenu__separator"></div><ul id="curated_content"><li><a class="navigation-submenu__link" href="/fr/images#curated-content" id="curated_sets">Sélection de séries</a></li><li><a class="navigation-submenu__link" href="/fr/collaboration/boards/AwxaA_jHWUyul-5RWiY4UA" id="signature_collection">Collection Signature</a></li><li><a class="navigation-submenu__link" href="/fr/collaboration/boards/wu0HHWcoZ0e_JoqU2nXTOg" id="essentials_collection">Collection Essentials</a></li><li><a class="navigation-submenu__link" href="/fr/diversit%C3%A9" id="essentials_collection">Images Diversité et inclusion</a></li></ul></div><div class="navigation-submenu__column"><div class="navigation-submenu__header">Recherches actuelles</div><div class="navigation-submenu__separator"></div><ul id="trending_searches"><li><a class="navigation-submenu__link" href="/fr/photos/summer">Summer</a></li><li><a class="navigation-submenu__link" href="/fr/photos/paris">Paris</a></li><li><a class="navigation-submenu__link" href="/fr/photos/maison">Maison</a></li><li><a class="navigation-submenu__link" href="/fr/photos/femme">Femme</a></li><li><a class="navigation-submenu__link" href="/fr/photos/formation">Formation</a></li><li><a class="navigation-submenu__link" href="/fr/photos/handicap">Handicap</a></li></ul></div><div class="navigation-submenu__column"><div class="navigation-submenu__header">Cat