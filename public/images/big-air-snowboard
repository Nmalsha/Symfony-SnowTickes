const editorTooltip = document.getElementsByClassName("editor-tooltip")[0];
const globalNavLinks = document.querySelectorAll('[data-trackNav]');
for (var i = 0; i < globalNavLinks.length; i++) {
  globalNavLinks[i].addEventListener('click', event => {
    if(event.currentTarget.href) event.preventDefault();
    if (window.combinedGtmTracking) {
      window.combinedGtmTracking.gtmPushTracking('navigationInteraction', {
        'navigation_interaction': 'Header_Link_Click',
        'navigation_selection': `sub_nav_video_editor_${event.currentTarget.dataset.tracknav}`,
        'ui_element_location': 'header'
      });
    }
    if(event.currentTarget.href) window.open(event.currentTarget.href, '_blank');
  });
}

editorTooltip.style.display = showTooltip ? "block" : "none";
if(showTooltip) {
  const createVideos = document.getElementsByClassName("video-editor")[0];
  const close = document.getElementsByClassName("close-tt")[0];
  const projLink = document.getElementsByClassName("projLink")[0];
  let waitForGlobal = (key, callback) => {
    if (window[key]) {
      callback();
    } else {
      setTimeout(() => {
        waitForGlobal(key, callback);
      }, 100);
    }
  };
  let track = (interaction) => {
    if (window.combinedGtmTracking) {
      window.combinedGtmTracking.gtmPushTracking('notificationTooltipInteraction', {
        'tooltip_interaction': interaction,
        'tooltip_selection': 'nav_tooltip_drafts_reminder',
        'ui_element_location': 'nav_header_tooltip_video_editor'
      });  
    }
  }
  waitForGlobal("combinedGtmTracking", () => { track('Viewed'); });
  // Needed as the tooltip and the create menu overlap with each other. 
  let closeTooltip = () => {
    localStorage.setItem('show_editor_draft_reminder', false);
    editorTooltip.style.display = "none";
  }
  if (close)
    close.addEventListener('click', event => {
      closeTooltip();
      track('Close');
    });
  if (projLink)
    projLink.addEventListener('click', event => {
      event.preventDefault();
      closeTooltip();
      track('Click'); 
      location.href = projLink.getAttribute('href');
    });
}</script><li class="icon-only boards-icon boards-icon-new" id="open_board"><a href="javascript:void(0);"><span alt="Tableaux" class="icon-new"></span><span class="action-label new">Tableaux </span><span class="carrot-container"><span class="carrot"></span></span></a><div class="boards-menu-container"></div></li><li class="thin-header right-off-canvas-toggle-menu" data-istock-user-id=""><figure class="notification-dot" ng-style="{&#39;opacity&#39;: displayRedDot ? &#39;1&#39; : &#39;0&#39;}"></figure><figure class="account"><img class="rest-state" src="/search/assets/static/istock_global-4918a622f02f75589638.svg#account-icon-mono" viewBox="0 0 100 100" /><img class="hover-state" src="/search/assets/static/istock_global-4918a622f02f75589638.svg#account-icon" viewBox="0 0 100 100" /></figure><span class="display-name">Compte</span><span class="carrot-container"><span class="carrot"></span></span></li><li class="wide-header"><a class="account" data-nav="nav=nav_SignIn" href="/fr/sign-in?returnurl=%2Ffr%2Fphotos%2Fbig-air-snowboard" login-link="" rel="nofollow">Identifiez-vous</a></li><li class="wide-header"><a class="account" data-nav="nav=nav_Join" href="/fr/join" rel="nofollow">S'inscrire</a></li></ul></div></nav><div class="dd-user-panel-menu right"><div class="close-panel"><figure class="general-icon-slider-close"></figure></div><ul class="not-remembered"><li><a data-nav="nav=nav_SignIn" href="/fr/sign-in?returnurl=%2Ffr%2Fphotos%2Fbig-air-snowboard" login-link="">Identifiez-vous</a></li><li><a data-nav="nav=nav_Join" href="/fr/join">S'inscrire</a></li><li class="mobile"><a data-nav="nav=nav_Account_video_templates" data-ui-location="header_side_panel_right" href="/fr/video-editor/templates">Modèles vidéo</a></li><li class="mobile"><a data-nav="nav=nav_Pricing" href="/fr/s%C3%A9lections-et-tarifs">Sélections et tarifs</a></li><li class="mobile"><a data-nav="nav=nav_YourBoards" href="/fr/collaboration/boards/default">Vos tableaux</a></li></ul></div><div><div class="